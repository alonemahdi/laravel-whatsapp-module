{
    "name": "vendor/whatsapp-module",
    "description": "WhatsApp Module for Laravel - A Composer package for sending WhatsApp messages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahdi Tanha",
            "email": "alonemahdi01@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\WhatsAppModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\WhatsAppModule\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\WhatsAppModule\\WhatsAppModuleServiceProvider"
            ],
            "aliases": {
                "WhatsApp": "Vendor\\WhatsAppModule\\Facades\\WhatsApp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

